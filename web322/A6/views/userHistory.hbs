<div class="row">
    <div class="col-md-12">
        <h2>{{session.user.userName}} &nbsp;(&nbsp;{{session.user.email}}&nbsp;)&nbsp;History</h2>
        <hr />
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Login&nbsp;Date/Time</th>
                        <th>Client&nbsp;Information</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each session.user.loginHistory}}
                    <tr>
                        <td>{{dateTime}}</td>
                        <td>{{userAgent}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>